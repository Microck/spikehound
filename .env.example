# App
APP_ENV=dev
LOG_LEVEL=INFO

# Azure (service principal auth recommended for automation)
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_SUBSCRIPTION_ID=
AZURE_AI_PROJECT_ENDPOINT=

# Azure OpenAI / Foundry (optional: enables LLM-backed diagnosis)
# NOTE: FOUNDRY_MODEL is the Azure OpenAI *deployment name* (not the base model name).
FOUNDRY_ENDPOINT=
FOUNDRY_MODEL=
FOUNDRY_API_KEY=
# Required for Structured Outputs (json_schema). JSON mode (json_object) works with older versions.
FOUNDRY_API_VERSION=2024-08-01-preview

# Slack
SLACK_WEBHOOK_URL=
SLACK_SIGNING_SECRET=

# Discord
DISCORD_WEBHOOK_URL=
DISCORD_WEBHOOK_USERNAME=
DISCORD_WEBHOOK_AVATAR_URL=
DISCORD_WEBHOOK_THREAD_ID=
DISCORD_WEBHOOK_TIMEOUT_SECONDS=10
DISCORD_WEBHOOK_MAX_RETRIES=2
DISCORD_WEBHOOK_BACKOFF_SECONDS=1
# Required for signed Discord interaction callbacks at /webhooks/discord/interactions
DISCORD_INTERACTIONS_PUBLIC_KEY=
